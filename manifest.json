{
    "name": "Web Capture",
    "description": "Web Scrapping Tool",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js",
        "type": "module",
        "js": ["pdfmake.js"]
    },
    "permissions": ["storage", "activeTab", "scripting", "unlimitedStorage" ],
    "action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "content_scripts": [{
        "matches": ["https://www.youtube.com/**"],
        "css": ["core.css","jquery-ui.css","jquery-ui.structure.css","jquery-ui.theme.css"],
        "js": ["core.js","html2canvas.min.js","jquery-1.12.4.js","jquery-ui-1.12.1.js","jspdf.umd.min.js"]
    }],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline' https://adservice.google.co.in/adsid/integrator.js?domain=stackoverflow.com;"
    }
}