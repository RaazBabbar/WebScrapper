{
    "name": "Web Capture",
    "description": "Web Scrapping Tool",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting", "unlimitedStorage" ],
    "action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "content_scripts": [{
        "matches": ["https://*/*", "https://13.94.233.140/aap-assement/"],
        "css": ["core.css","jquery-ui.css","jquery-ui.structure.css","jquery-ui.theme.css"],
        "js": ["core.js","jquery-1.12.4.js","jquery-ui-1.12.1.js"]
    }]
}