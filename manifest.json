{
    "name": "Web Scrapper",
    "description": "Web Scrapper Tool",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting", "unlimitedStorage" ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/get_started16.png",
            "32": "/images/get_started32.png",
            "48": "/images/get_started48.png",
            "128": "/images/get_started128.png"
        }
    },
    "options_page": "options.html",
    "content_scripts": [{
        "matches": ["https://*/*"],
        "css": ["core.css","jquery-ui.css","jquery-ui.structure.css","jquery-ui.theme.css"],
        "js": ["core.js","jquery-1.12.4.js","jquery-ui-1.12.1.js"]
    }]
}